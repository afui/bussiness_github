.Info 
{
    padding_bottom: 10px;
	PADDING-RIGHT: 10px; 
	padding-top: 10px; 
	PADDING-LEFT: 10px; 
	OVERFLOW-Y: auto; 
	font-size: 12px;
	SCROLLBAR-FACE-COLOR: #F9ECE3; 
   SCROLLBAR-HIGHLIGHT-COLOR: #E7D7BE; 
   SCROLLBAR-SHADOW-COLOR: #E7D7BE; 
   SCROLLBAR-3DLIGHT-COLOR:#E7D7BE; 
   TEXT-INDENT: 24px; 
   LINE-HEIGHT: 25px; 
   SCROLLBAR-ARROW-COLOR: #E7D7BE;  
   SCROLLBAR-TRACK-COLOR: #E7D7BE; 
   SCROLLBAR-DARKSHADOW-COLOR:#E7D7BE; 
   HEIGHT: 342px;  
   TEXT-ALIGN: right
}

.smallinput
{
	BACKGROUND-COLOR: #FFFFFF; 
	BORDER-BOTTOM: #9F9F9F 1px solid; 
	BORDER-LEFT: #676767 0px solid; 
	BORDER-RIGHT: #676767 0px solid; 
	BORDER-TOP: #676767 0px solid;
}


.Btn
{
    BACKGROUND-COLOR: menu;
    BORDER-BOTTOM: buttonface 1px solid;
    BORDER-LEFT: buttonface 1px solid;
    BORDER-RIGHT: buttonface 1px solid;
    BORDER-TOP: buttonface 1px solid;
    HEIGHT: 23px;
    POSITION: absolute;
    TOP: 1px;
    WIDTH: 23px
}
input.but {
	border-right: powderblue 1px ridge; border-top: powderblue 1px ridge; font-size: 9pt; border-left: powderblue 1px ridge; border-bottom: powderblue 1px ridge; height: 21px; background-color: #b8b8e1;cursor:hand
}
input.but_color {
	border-right: powderblue 1px ridge; border-top: powderblue 1px ridge;
	 font-size: 9pt; border-left: powderblue 1px ridge; border-bottom: powderblue 1px ridge; 
	 height: 21px; background-color: #b8b8e1;
	 color:Red;
	 cursor:hand
}
input.butt {
	border-right: powderblue 1px ridge; border-top: powderblue 1px ridge; font-size: 9pt; border-left: powderblue 1px ridge; width: 120px; border-bottom: powderblue 1px ridge; height: 20px; background-color: #cccc99
}
.table {
	border-right: #000000 1px solid; background: #d6d3d6; border-bottom: #000000 1px solid
}
.undisplay {
	display: none
}
body,p,tr,td,input,div {
	font-size: 9pt
}
.input {
	border-right: #000000 1px solid; border-top: #000000 1px solid; font-size: 9pt; border-left: #000000 1px solid; border-bottom: #000000 1px solid
}
.jp_link {
	font-size: 9pt; font-family: verdana; text-decoration: underline
}
.title {
	font-weight: bold; font-size: 12pt; color: #000000
}
.select {
	border-right: #000000 1px solid; border-top: #000000 1px solid; font-size: 9pt; border-left: #000000 1px solid; border-bottom: #000000 1px solid
