.contact_mobile{
  border:1px solid #cae0f5;
  background-position:0 -313px;
  background-repeat:repeat-x;
  height:70px;
  overflow:hidden;
}
.prop_top_mobile{width:348px;margin:5px 0 0 0;}
.contact_mobile .tel_border{
  height:68px;
  border:1px solid #fff;
}
.contact_mobile .tel_mobile{
  float:left;
  width:45px;
  height:45px;
  overflow:hidden;
  background-position:0 0;
  margin:11px 15px 0 15px;
  _margin:11px 15px 0 8px;
}
.contact_mobile .tel_border strong{
  font-family: Tahoma;
  font-size: 26px;
  line-height: 100%;
  display:block;
  height:28px;
  margin-top:10px;
}
.contact_height{ line-height:70px;}
.contact_mobile_down{height:46px;}
.contact_mobile_down .tel_border{ height:44px;}

.prop_info_tools{height:26px;border-top:0px;margin:15px 0 0;}
.pho_ul_tools li{ display:inline-block; margin-right:11px;*display:inline;*zoom:1}
.pho_ul_tools .li_line{height:13px;font-size:0px;border-left:#dadada solid 1px;vertical-align: middle}
.pho_ul_tools li a{}
.pho_ul_tools li a:hover{text-decoration:none;}
.pho_ul_tools li .icon_mobile{ padding:0 5px; background-position:-92px -106px; margin-right:5px;}
.pho_ul_tools li .icon_collection{ padding:0 6px; background:url( ../images/fav_sprite.gif ) no-repeat 0px -3px; margin-right:5px;}
.pho_ul_tools li .icon_report{ padding:0 5px; background-position:-60px -106px; margin-right:5px;}
.pho_ul_tools span.icon_report{
  background-position:0 -1028px;
  height:20px;
  line-height:20px;
  *line-height:22px;
  padding-left:18px;
  padding-right:0;
  margin-right:0;
  margin-top:4px;
  overflow:hidden;
  display:block;
}
.viewprop_top_crumbs .crumbs_r_mob a i.phone_icon, a i.phone_icon{ padding:3px 4px; background-position:0 -103px; margin-right:4px;}

/*介绍选项卡*/
.current1 { color:#FFFFFF;cursor:pointer; text-align:center; background:url(../images/lman-bg1.jpg); }
.htLink1 { color:#005D88;cursor:pointer; text-align:center; background:url(../images/lman-bg2.jpg); }
.htActive1{ color:#FFFFFF;cursor:pointer; text-align:center; background:url(../images/lman-bg1.jpg); }


/*问答页面*/
.qa-layer{border:#99C8FB 1px solid;border-top:#6FB2F9 1px solid;padding:18px 28px 29px 28px;background:#fff url(../images/layer_bg.png) repeat-x 0 0;}
.qa-content dl dt{background:url(../images/detail_icons.gif) no-repeat 0 -486px;line-height:24px;font-size:14px;font-weight:bold;padding:0 0 10px 28px;}
.qa-content dl dt.plus_icons{background:url(../images/detail_icons.gif) no-repeat 0 -548px;}
.qa-content dl dt h1{font-size:14px;}
.qa-content dl dd{height:16px;line-height:16px;padding-bottom:5px;border-bottom:#ccc 1px dashed;color:#7e7e7e;}
.qa-content dl dd em,.qa-content dl dd span{line-height:14px;height:16px;}
.qa-content dl dd em{font-style:normal;}
.qa-content dl dd span.split{color:#e7e7e7;margin:0 5px;}
.qa-details,.answer-details{word-wrap:break-word;word-break:break-all;font-size:14px;line-height:24px;padding:15px 0 0 0;}

.answer-layer{padding:20px 20px 25px 28px;}
.answer-layer h2{background:url(../images/detail_icons.gif) no-repeat;height:22px;line-height:20px;font-size:14px;text-indent:30px;}
.best-layer{border:#FCB120 1px solid;border-top:#FCB120 1px solid;background:url(../images/best_answer_bg01.png) repeat-x 0 0;}
.best-layer h2{background-position:0 -392px;}
.plusfine-layer,.recommend-layer{border:#F8CD7A 1px solid;border-top:#FBBF4B 1px solid;background:url(../images/best_answer_bg02.png) repeat-x 0 0;}
.plusfine-layer h2{background-position:0 -424px;}
.recommend-layer h2{background-position:0 -456px;}
.details-boxer{padding:14px 0 0 0;}
.answer-layer .answer-details{padding:0;}

/*地图找房页面*/
.hy_header_map { width:100%;height:60px; margin: auto; background:url(../images/bg01.gif) repeat-x;}
.baidu_main{   
    position:relative;
    z-index:1;
	width: 100%;
	height: 100%;
}
.baidu_map{
    position:relative;
    overflow:hidden;
    width: 100%;
	height: 100%;
    z-index:10;
}
.bg_shadow{
    position: absolute;
    width: 100%;
    z-index:999;
    top:0px;
    left:0px;
    font-size:0px;
    line-height:0px;
    height:3px;
    border-top: #b8b8b8 solid 1px;
    background: url("../images/lineShadow.png") repeat-x left top;
    filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(sizingMethod="scale", src='../images/lineShadow.png');
    *background:none;
    _margin-left: -190px;
}
.baidu_map .bg_shadow_l{
    position: absolute;
    width:3px;
    z-index:200;
    top:0px;
    left:0px;
    font-size:0px;
    line-height:0px;
    height: 100%;
    border-left: #b8b8b8 solid 1px;
    background: url("../images/lineShadow_l.png") repeat-y left top;
    filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(sizingMethod="scale", src='../images/lineShadow_l.png');
    *background:none;
}

.map_l_dl{
    margin:0 0 12px -10px;
    width:180px;
    line-height:22px;
    overflow:auto;
    _display:inline-block;
}
.map_l_dl dt{
    float:left;
    width:80px;
    font-weight:bold;
    font-size:14px;
    margin:0 0 0 10px;
    _margin:0 0 0 5px;
}
.map_l_dl dd{
    float:left;
    width:80px;
    margin:0 0 0 10px;
    _margin:0 0 0 5px;
}
.map_l_dl dd a.selected{
    font-weight:bold;
    color:#EB6100;
}

.marker {
    position: absolute;
    display: none;
    /*width: 45px;*/
    height: 40px;
    padding: 0;
    color: white;
    text-align: center;
    background-image: url("../images/marker.png");
    background-repeat:no-repeat;
}
.marker {
    _background: none;
    _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader (src='../images/marker.png', sizingMethod='crop');
}
.marker .marker_bg{
    margin-left: 10px;
    white-space:nowrap;
    height: 28px;
    line-height: 28px;
    padding: 0 8px 0 0;
    font-size: 12px;
    cursor: pointer;
    background-image: url('../images/marker_bg.gif');
    background-repeat:no-repeat;
    background-position:right 0px;
    _float:left;
    _margin-left:5px;
}

.marker_hover {
    z-index: 4;
    background-image: url("../images/marker-h.png");
}

.marker_hover {
    _background: none;
    _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader (src='../images/marker-h.png', sizingMethod='crop');
}
.marker_bg{
    background-image: url("../images/marker-h-l.gif");
    background-repeat:no-repeat;
    background-position:right 0px;
}

.marker_activa {
    z-index: 4;
    background-image: url("../images/marker-h.png");
}
* html .marker_activa {
    background: none;
    filter:progid:DXImageTransform.Microsoft.AlphaImageLoader (src='../images/marker-h.png', sizingMethod='image');
}
.pull-left {
  padding:400px 500px 0 0px;
}