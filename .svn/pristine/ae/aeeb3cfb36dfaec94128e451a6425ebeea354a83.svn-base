    <style type="text/css">
        body
        {
            overflow-x: hidden;
            background: none;
        }
        #zlws_page
        {
            width: 100%;
        }
        /*资料完善-->标题*/
        #zlws_page .wsbt
        {
            color: #333;
            font-family: "微软雅黑";
            font-size: 14px;
            line-height: 35px;
            float: left;
            height: 35px;
            width: auto;
        }
        
        
        /*资料完善-->个人信息*/
        #zlws_page #zlxx_border
        {
            width: auto;
            border-left: 1px solid #fdd1aa;
            border-right: 1px solid #fdd1aa;
        }
        /*边框*/
        #zlxx_border .user_xx
        {
            width: auto;
            padding: 15px 30px;
        }
        #zlxx_border .user_xx .user_img
        {
            padding: 5px;
            border: 1px solid #ccc;
            background: #fff;
            float: left;
        }
        #zlxx_border .user_xx .user_img img
        {
            width: 100%;
        }
        #zlxx_border .user_xx .user_bt
        {
            float: left;
            width: 100%;
            font-size: 14px;
            font-weight: bold;
            color: #fe8417;
            border-bottom: 1px solid #e5e5e5;
            line-height: 35px;
        }
        
        
        /*资料完善-->用户基本信息、旅行社管理*/
        #zlws_page #box_zlxx
        {
            width: auto;
            padding: 0px;
            border: 1px solid #fdd1aa;
            border-top: none;
            float: none;
        }
        /*边框*/
        #box_zlxx td
        {
            line-height: 40px;
        }
        #user_table td
        {
            text-align: left;
            width: 25%;
        }
        #box_zlxx .xbt
