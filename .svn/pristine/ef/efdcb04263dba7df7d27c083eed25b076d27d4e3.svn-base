        /*中间图片通用设置*/
        .bg span
        {
            display: block;
            width: 48px;
            height: 68px;
            background: url(images/zc.png) no-repeat;
            position: absolute;
        }

        .bg b
        {
            display: block;
            width: 61px;
            height: 95px;
            background: url(images/sos.png) no-repeat;
            position: absolute;
        }

    /*各坐标点位置*/
    .A1
    {
        left: 21px;
        top: 10px;
    }

    .A2
    {
        left: 121px;
        top: 50px;
    }

    .A3
    {
        left: 221px;
        top: 90px;
    }

    .A4
    {
        left: 321px;
        top: 130px;
    }

    .B1
    {
        left: 221px;
        top: 110px;
    }

    .B2
    {
        left: 221px;
        top: 150px;
    }

    .B3
    {
        left: 221px;
        top: 190px;
    }

    .B4
    {
        left: 421px;
        top: 330px;
    }
</style>

<body>
    <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
            <td valign="top" bgcolor="#CFF1F3">
                <table border="0" cellspacing="0" cellpadding="0">
                    <tr>
                        <td align="center" valign="top" class="p5">
